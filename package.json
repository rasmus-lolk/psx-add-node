{
  "name": "smooth-tools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "lint": "ng lint",
    "test": "ng test",
    "prebuild": "npm run generatepath",
    "postbuild": "npm run package",
    "prepackage": "npm run copy-manifest && npm run copy-license && npm run write-requirements",
    "package": "package-urcap dist target",
    "copy-manifest": "copy-file manifest.yaml dist/",
    "write-requirements": "write-ur-packages ./package.json dist/requirements.txt",
    "copy-license": "copy-file LICENSE dist/",
    "generatepath": "generate-manifest-constants ./manifest.yaml",
    "install-urcap": "install-urcap 'target/smooth-tools*.urcapx'",
    "delete-urcap": "delete-urcap smooth-robotics smooth-tools"
  },
  "private": true,
  "license": "Apache-2.0",
